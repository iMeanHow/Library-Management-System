<h1>View Librarian Requests</h1>
<% if current_user %>
Hi,<%= current_user.name %> ! Logged in as <%= current_user.email %>.
<%= link_to "Log Out", logout_path %><br>
<%= link_to "Home", root_url %><br><br>
<%else %>
<%= link_to "Sign Up", signup_path %> or
<%= link_to "Log In", login_path %><br><br>
<% end %>

<p id="notice"><%= notice %></p><br>
<% @librariansNeedtoApprove.each do |user| %>
  <%= user.email %> <%= link_to "Approve", approve_librarian_requests_path(email: user.email) %>
<% end %>

<!--<b>this is librarian request view</b>-->